{
    "name": "xsolve-pl/face-validator-bundle",
    "description": "Symfony3 bundle for validating face on picture using MS Azure Face API",
    "license": "MIT",
    "homepage": "https://xsolve.software",
    "type": "symfony-bundle",
    "keywords": ["face", "validator", "detect", "recognition", "machine learning", "azure"],
    "authors": [
        {
            "name": "Pawel Krynicki",
            "email": "pawel.krynicki@xsolve.software"
        },
        {
            "name": "Michal Nicinski",
            "email": "michal.nicinski@xsolve.software"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "~6.3",
        "myclabs/php-enum": "~1.5",
        "symfony/http-kernel": "3.3.*",
        "symfony/config": "3.3.*",
        "symfony/dependency-injection": "3.3.*",
        "symfony/validator": "3.3.*",
        "symfony/property-access": "3.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "symfony/framework-bundle": "3.3.*",
        "symfony/yaml": "3.3.*"
    },
    "autoload": {
        "psr-4": {
            "XSolve\\": "src/XSolve"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
